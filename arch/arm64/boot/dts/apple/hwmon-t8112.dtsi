&smc {
	macsmc-hwmon {
		apple,temp-keys {
			temp-TSCD {
				apple,key-id = "TSCD";
				apple,key-desc = "SoC Die Temp";
			};
			temp-Tp00 {
				apple,key-id = "Tp00";
				apple,key-desc = "Performance Core 1 Temp";
			};
			temp-Tp08 {
				apple,key-id = "Tp08";
				apple,key-desc = "Performance Core 2 Temp";
			};
			temp-Tp0e {
				apple,key-id = "Tp0e";
				apple,key-desc = "Performance Core 3 Temp";
			};
			temp-Tp0m {
				apple,key-id = "Tp0m";
				apple,key-desc = "Performance Core 4 Temp";
			};
			/* no idea what monitored by this */
			temp-Tp0a {
				apple,key-id = "Tp0a";
			};
			temp-Tp0b {
				apple,key-id = "Tp0b";
			};
			temp-Tp0c {
				apple,key-id = "Tp0c";
			};
		};
	};
};
